    "eslint-plugin-prettier": "^2.2.0",
    "express": "^4.16.2",
    "html-webpack-plugin": "^3.0.6",
    "husky": "^0.14.3",
    "jest": "^22.0.6",
    "js-yaml": "^3.10.0",
    "lint-staged": "^4.2.1",
    "nsp": "^2.6.3",
    "prettier": "^1.7.0",
    "request": "^2.83.0",
    "underscore": "^1.8.3",
    "underscore.string": "^3.3.4",
    "webpack": "^4.1.0",
    "webpack-cli": "^2.0.10",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0",
    "yo": "^2.0.1"
  },
  "scripts": {
    "build:dev": "webpack --mode development --config config/webpack.config.dev.js",
